  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bdbcb260"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#bdbcb2"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2d79c5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#d97757"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7d4a38"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#2d79c5"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character"
      ],
      "settings": {
        "foreground": "#d97757"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#2d79c5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#d97757"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7d4a38"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d97757"
      }
    }
  ]
} 