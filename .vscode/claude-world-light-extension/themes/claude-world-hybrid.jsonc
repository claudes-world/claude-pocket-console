    "editorGutter.background": "#30302e",
    "editorGutter.modifiedBackground": "#2d79c5",
    "editorGutter.addedBackground": "#7d4a38",
    "editorGutter.deletedBackground": "#d97757",
    
    // Terminal - LIGHT (Claude Light Theme Colors)
    "terminal.background": "#ffffff",
