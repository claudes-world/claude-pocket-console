    "terminal.ansiBrightGreen": "#DE7356",
    "terminal.ansiBrightYellow": "#f3e9d7",
    "terminal.ansiBrightBlue": "#33B7FF",
    "terminal.ansiBrightMagenta": "#D6B9DB",
    "terminal.ansiBrightCyan": "#A9CCE3",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.selectionBackground": "#DE735630",
    "terminal.border": "#A9CCE3",
    "terminalCursor.background": "#ffffff",
    "terminalCursor.foreground": "#433884",
    
    // Chat - Dark
    "chat.requestBackground": "#30302e",
    "chat.requestBorder": "#2d79c540",
    "chat.responseBackground": "#1e1e1c",
    "chat.responseBorder": "#bdbcb240",
    "chat.slashCommandBackground": "#7d4a3820",
    "chat.slashCommandForeground": "#bdbcb2",
    
    // Notifications - Dark
    "notificationCenter.border": "#2d79c540",
    "notificationCenterHeader.background": "#30302e",
    "notificationCenterHeader.foreground": "#bdbcb2",
    "notifications.background": "#1e1e1c",
    "notifications.foreground": "#bdbcb2",
    "notifications.border": "#2d79c540",
    "notificationLink.foreground": "#2d79c5",
    
    // Badge - Dark
    "badge.background": "#2d79c5",
    "badge.foreground": "#ffffff",
    
    // Misc - Dark
    "focusBorder": "#2d79c5",
    "foreground": "#bdbcb2",
    "descriptionForeground": "#bdbcb260",
    "errorForeground": "#d97757",
    "icon.foreground": "#bdbcb2",
    "selection.background": "#7d4a3830",
    "widget.shadow": "#00000060",
    "toolbar.hoverBackground": "#2d79c520",
    "toolbar.activeBackground": "#7d4a3830"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bdbcb260"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#bdbcb2"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2d79c5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#d97757"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7d4a38"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#2d79c5"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character"
      ],
      "settings": {
        "foreground": "#d97757"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#2d79c5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#d97757"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7d4a38"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d97757"
      }
    }
  ]
} 